---
import Page from './Page.astro';

export interface Props {
  title: string;
  jam?: string;
  description?: string;
}

const { 
  title, 
  jam = '',
  description = 'Tired Gaming is a host of game and game asset jams'
} = Astro.props;
---

<Page title={title} jam={jam} description={description}>
  <link rel="stylesheet" href="/tired_gaming/jam.css" slot="head">
  <slot />
</Page>
